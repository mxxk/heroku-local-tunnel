#!/usr/bin/execlineb -S0

multisubstitute
{
    importas -D "" PORT PORT
    importas -D "" TUNNELTO_ALLOWED_HOSTS TUNNELTO_ALLOWED_HOSTS
    importas -D "" TUNNELTO_CTRL_PORT TUNNELTO_CTRL_PORT
    importas -D "" TUNNELTO_TUNNEL_PORT TUNNELTO_TUNNEL_PORT
}
emptyenv -p
export PORT ${PORT}
export TUNNELTO_ALLOWED_HOSTS ${TUNNELTO_ALLOWED_HOSTS}
export TUNNELTO_CTRL_PORT ${TUNNELTO_CTRL_PORT}
export TUNNELTO_TUNNEL_PORT ${TUNNELTO_TUNNEL_PORT}
haproxy -f /app/haproxy.cfg
